<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.ougnt.period_manager"
          android:versionCode="27"
          android:versionName="1.16.2">
    <uses-sdk android:minSdkVersion="16"/>
    <uses-permission android:name="android.permission.INTERNET" />
    <!--<application android:label="@string/app_name" android:icon="@drawable/icon" android:debuggable="false">-->
    <application android:label="@string/app_name" android:icon="@drawable/icon">
    <activity android:name=".activity.InitialActivity"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    <activity android:name=".activity.CommentActivity"/>
    <activity android:name=".activity.HelpActivity"/>
    <activity android:name=".activity.MenuActivity" android:theme="@style/Theme.Transparent"/>
    <activity android:name=".activity.SummaryActivity" android:theme="@style/Theme.Transparent"/>
    <activity android:name=".activity.SettingActivity"/>
    <activity android:name=".activity.CalendarActivity"/>
    <activity android:name=".activity.MonthViewActivity" android:theme="@style/Theme.Transparent"/>
    <receiver android:name="com.ougnt.period_manager.event.BroadcastNotificationPublisher">
        <intent-filter>
            <action android:name="com.ougnt.period_manager.activity.InitialActivity"/>
        </intent-filter>
    </receiver>

    </application>
</manifest>
